generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Receipt {
  id           String   @id @default(uuid())
  name         String
  ex           String
  timeInvested Int
  moneySpent   Int
  emotional    Int
  betrayal     Int
  reason       String
  compensation Int
  createdAt    DateTime  @default(now())
  updateAt     DateTime  @updatedAt
}